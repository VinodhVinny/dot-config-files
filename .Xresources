URxvt.font: xft:monospace:size=9:antialias=true:hinting=true,\
			xft:Symbola
URxvt.scrollBar: false
URxvt.saveLines: 10000
URxvt.termName: rxvt-256color
URxvt.xftAntialias: true
xft.antialias: true
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

!! Extensions
URxvt.perl-ext-common: default,matcher,resize-font,clipboard
! clickable urls
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.colorUL: #00ff00
urxvt.keysym.M-u: perl:url-select:select_next

! resize-font
URxvt.keysym.C-k: resize-font:smaller
URxvt.keysym.C-j: resize-font:bigger
! clipboard
URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste

!! color scheme

! special
*.foreground:   #00cc00
*.background:   [90]#080808
*.cursorColor:  #babdb6

! black
*.color0:       #2e3436
*.color8:       #555753

! red
*.color1:       #cc0000
*.color9:       #ef2929

! green
*.color2:       #4e9a06
*.color10:      #8ae234

! yellow
*.color3:       #c4a000
*.color11:      #fce94f

! blue
*.color4:       #3465a4
*.color12:      #729fcf

! magenta
*.color5:       #75507b
*.color13:      #ad7fa8

! cyan
*.color6:       #06989a
*.color14:      #34e2e2

! white
*.color7:       #d3d7cf
*.color15:      #eeeeec

